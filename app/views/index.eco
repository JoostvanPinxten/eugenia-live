<ul class="thumbnails">
  <% for drawing in @drawings: %>
  <li class="span4">
    <a href="#/drawings/<%=drawing.id%>" class="thumbnail" style="text-align:center;">
      <img src="http://placehold.it/520x360" alt="">
      <h5><%=drawing.id%></h5>
    </a>
  </li>
  <% end %>
  <li class="span4">
    <div class="thumbnail" style="padding:114px 0;text-align:center;">
      <button id="new" class="btn">New Drawing</button>
    </div>
  </li>
</ul>

